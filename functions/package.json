{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "watch": {
    "start": "*.js"
  },
  "scripts": {
    "lint": "eslint .",
    "serve": "functions-framework --target=createExternalAccount",
    "shell": "firebase functions:shell",
    "start": "npm run serve",
    "watch": "npm-watch start",
    "deploy": "firebase deploy --only functions:createExternalAccount",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.6.0",
    "busboy": "^0.3.1",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.1.0",
    "stripe": "^8.67.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.1.6",
    "npm-watch": "^0.6.0"
  },
  "private": true
}
